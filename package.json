{
  "name": "setup-openhsp",
  "version": "0.1.0",
  "description": "Set up your GitHub Actions workflow with a specific version of OpenHSP.",
  "main": "dist/index.js",
  "scripts": {
    "build-setup": "ncc build -o dist/setup src/setup.ts -m",
    "build-cache": "ncc build -o dist/cache src/cache.ts -m",
    "build": "rimraf dist && npm run build-setup && npm run build-cache",
    "format": "prettier --write src/**/*.ts",
    "check-format": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "check": "npm run check-format && npm run lint",
    "build-readme": "gha-docgen",
    "test": "jest"
  },
  "author": "BonyChops",
  "license": "MIT",
  "devDependencies": {
    "@actions/tool-cache": "^2.0.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "gha-docgen": "^1.0.1",
    "jest": "^29.7.0",
    "prettier": "3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@supercharge/promise-pool": "^3.1.0"
  }
}
